<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="vike" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="./build/bin/vike.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="./build/vike" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log -na -ns -nd" />
				<Option host_application="../../../devel/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Winvalid-pch" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DwxUSE_UNICODE" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
				</Linker>
			</Target>
			<Environment>
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add directory="$(#CB_PATH)/src/sdk/wxscintilla/include" />
			<Add directory="$(#CB_PATH)/src/include" />
			<Add directory="$(#WX.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
			<Add directory="$(#WX)/contrib/include" />
			<Add directory="$(#WX.include)" />
		</Compiler>
		<Linker>
			<Add library="wxmsw28u_gcc_cb" />
			<Add library="wxscintilla_cb" />
			<Add library="codeblocks" />
			<Add directory="$(#WX.lib)/gcc_dll$(WX_CFG)" />
			<Add directory="$(#CB_PATH)/src/devel" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 .\build\bin\vike.zip manifest.xml" />
			<Add after="zip -j9 .\build\vike.cbplugin .\build\bin\vike.dll .\build\bin\vike.zip" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="cbvike.cpp" />
		<Unit filename="cbvike.h" />
		<Unit filename="cbvike_plugin.cpp" />
		<Unit filename="cbvike_plugin.h" />
		<Unit filename="debugging.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="vifunc.cpp" />
		<Unit filename="vifunc.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<DoxyBlocks>
				<comment_style block="0" line="0" />
				<doxyfile_project />
				<doxyfile_build />
				<doxyfile_warnings />
				<doxyfile_output />
				<doxyfile_dot />
				<general />
			</DoxyBlocks>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
